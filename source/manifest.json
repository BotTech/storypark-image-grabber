{
  "name": "Storypark Image Grabber",
  "version": "0.0.1",
  "description": "A browser extension that adds a button to download your child's photos from Storypark.",
  "homepage_url": "https://github.com/BotTech/storypark-image-grabber",
  "manifest_version": 3,
  "minimum_chrome_version": "100",
  "browser_specific_settings": {
	"gecko": {
	  "id": "awesome-extension@notlmn.github.io",
	  "strict_min_version": "100.0"
	}
  },
  "permissions": [
	"downloads"
  ],
  "content_scripts": [
	{
	  "matches": [
		"https://app.storypark.com/*"
	  ],
	  "js": [
		"content.ts"
	  ],
	  "css": [
		"content.css"
	  ]
	}
  ],
  "background": {
	"service_worker": "background.ts",
	"type": "module"
  }
}
